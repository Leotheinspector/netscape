{
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "^/netscape(?:/.*)?$",
      "dest": "server.js"
    },
    {
      "src": "^/$",
      "dest": "public/index.html"
    },
    {
      "src": "^/(.*)",
      "dest": "public/$1"
    }
  ]
}
